{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/chat",
            "dest": "index.js"
        }
    ],
    "env": {
        "DEEPSEEK_API_KEY": "@deepseek_api_key"  // Укажите имя переменной окружения в Vercel
    }
}
